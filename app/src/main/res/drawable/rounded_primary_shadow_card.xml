<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#03D776FE" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <solid android:color="#0FD776FE" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#17D776FE" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp">
        <shape android:shape="rectangle">
            <solid android:color="#1CD776FE" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp">
        <shape android:shape="rectangle">
            <solid android:color="#24D776FE" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="3dp">
        <shape android:shape="rectangle">
            <gradient
                android:angle="135"
                android:centerColor="@color/color_primary"
                android:endColor="@color/color_primary_dark"
                android:startColor="@color/color_primary" />

            <corners android:radius="8dp" />
        </shape>
    </item>
</layer-list>
